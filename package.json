{
  "name": "ls5-ui-poc",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/react-intl": "^2.3.14",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-intl": "^2.7.2",
    "react-redux": "^6.0.0",
    "react-scripts-ts": "^4.0.8",
    "redux": "^4.0.1",
    "redux-form": "^8.1.0"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "npm run extract:messages && npm run manage:translations && npm run build:production",
    "build:production": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "test:bash": "winpty node.cmd ./node_modules/react-scripts-ts/bin/react-scripts-ts.js test --env=jsdom",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,json,css}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --ignore-pattern src/registerServiceWorker.ts",
    "---i18n stuff---": "",
    "extract:messages": "ts-node scripts/extract-messages \"src/**/*.tsx\" src/i18n/extracted",
    "manage:translations": "ts-node scripts/translationRunner.ts src/i18n/extracted src/i18n/locales"
  },
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "eslint": "^5.6.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-react": "^7.12.0",
    "eslint-plugin-typescript": "^0.14.0",
    "glob-promise": "^3.4.0",
    "prettier": "^1.15.3",
    "react-intl-translations-manager": "^5.0.3",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.1",
    "typescript-eslint-parser": "^21.0.2",
    "typescript-react-intl": "^0.3.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
